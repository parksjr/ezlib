{
  "name": "ezlib",
  "version": "1.0.0",
  "description": "Easy static methods using zlib library",
  "main": "lib/ezlib.js",
  "scripts": {
    "start": "npm run test",
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "test": "mocha --reporter progress src/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-lib": "rimraf ./lib && mkdir lib",
    "prebuild": "npm-run-all clean-lib security-check test lint",
    "build": "babel -d ./lib ./src --ignore \"**/*.test.js\",\"**/testSetup.js\""
  },
  "keywords": [],
  "author": "Michael Parks",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/parksjr/ezlib/issues"
  },
  "homepage": "https://github.com/parksjr/ezlib",
  "dependencies": {
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "cross-env": "3.1.3",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "rimraf": "2.5.4",
    "surge": "0.18.0"
  }
}
